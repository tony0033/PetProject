select * from member;

create table member( 
id varchar(30) primary key,
pw varchar(30),
nickname varchar(30) not null unique,
name varchar(20),
tel varchar(30),
address varchar(150),
birth varchar(20),
pethospital varchar(150)
);

create table product (
name varchar(1000),
price varchar(1000),
img varchar(1000),
other varchar(1000),
link varchar(1000),
num int
other varchar(1000)
);

create table diary (
no int primary key, 
title varchar(50),
calendar varchar(30),
content varchar(1000),
id varchar(30)
);

create table bbs (
bNum number,
bId varchar(30),
bTitle varchar(50),
bDate varchar(30),
bCategory varchar(30),
bContent varchar(1000),
bView number
);

create table interest (
num int,
id varchar(100)
);

create table bbsComment (
cNum number,
bTitle varchar(50),
cId varchar(30),
cComment varchar(1000),
cDate varchar(30)
);

create table video(
no number,
src varchar(500),
title varchar(500),
views varchar(100),
type number
);

create table column(
no number
addr varchar(500),
title varchar(500),
);

create sequence bNum
start with 0
increment by 1
nomaxvalue
minvalue -1;

create sequence cNum
start with 0
increment by 1
nomaxvalue
minvalue -1;